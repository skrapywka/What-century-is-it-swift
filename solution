func whatCentury(_ year: String) -> String {
    if let yearInt = Int(year) {
     let century = (yearInt + 99) / 100
     var ordNum: String = ""

      switch century % 10 {
      case 1: ordNum += "st"
      case 2: ordNum += "nd"
      case 3: ordNum += "rd"
      default: ordNum += "th"
        }
        return "\(century)\(ordNum)"
    } else {
        return "Error"
    }
}
